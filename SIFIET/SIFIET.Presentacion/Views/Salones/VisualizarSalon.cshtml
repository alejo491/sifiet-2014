@model SIFIET.GestionInfraestructura.Datos.Modelo.SALON

@{
    ViewBag.Title = "Detalles";
}

<h2>Visualizar</h2>

<div>
    <h4>Salon</h4>
    <hr />
    <dl>
        <dt>
            Nombre
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NOMBRESALON)
        </dd>
        <dt>
            Faculdad
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NombreFacultad)
        </dd>
        <dt>
            Estado
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ESTADOSALON)
        </dd>
       
    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "ModificarSalon", new { idSalon = Model.IDENTIFICADORSALON }) |
    @Html.ActionLink("Volver a la Lista", "Index")
</p>
