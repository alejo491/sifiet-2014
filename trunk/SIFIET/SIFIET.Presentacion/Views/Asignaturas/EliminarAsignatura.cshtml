@model SIFIET.GestionProgramas.Datos.Modelo.ASIGNATURA

@{
    ViewBag.Title = "Eliminar Asignatura";

}

<h2>Eliminar Asignatura</h2>

<div>
    <h4>Asignatura</h4>
    <hr />
    <dl>
        <dt>
            Codigo
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CODIGOASIGNATURA)
        </dd>

        <dt>
            Plan de Estudio
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NombrePlaneEstudio)
        </dd>
        <dt>
            Nombre
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NOMBREASIGNATURA)
        </dd>

        <dt>
            Corequisitos
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CORREQUISITOSASIGNATURA)
        </dd>
        <dt>
            Prerequisitos
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PREREQUISITOSASIGNATURA)
        </dd>

        <dt>
            Semestre
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SEMESTREASIGNATURA)
        </dd>
        <dt>
            Creditos
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CREDITOSASIGNATURA)
        </dd>

        <dt>
            Modalidad
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MODALIDADASIGNATURA)
        </dd>
        <dt>
            Clasificacion
        </dt>
        <dd>
            @Html.DisplayFor(model => model.CLASIFICACIONASIGNATURA)
        </dd>
        <dt>
            Estado
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ESTADOASIGNATURA)
        </dd>
        <dt>
            Descripcion
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DESCRIPCIONASIGNATURA)
        </dd>


    </dl>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div>
            <input type="submit" value="Eliminar" onclick="return confirmarEliminarAsignatura()" /> |
            <input type="submit" value="Cancelar" onclick="return confirmSalirAsignatura();" />
        </div>
    }
</div>

